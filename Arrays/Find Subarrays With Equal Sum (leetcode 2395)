// java

class Solution {
    public boolean findSubarrays(int[] nums) {
        int n=nums.length;
        Map<Integer,Integer> map=new HashMap<>();

        for(int i=0; i<n-1; i++){
            int sum=nums[i]+nums[i+1];
            if(map.containsKey(sum)) return true;
            map.put(sum,0);
        }

        return false;
    }
}
