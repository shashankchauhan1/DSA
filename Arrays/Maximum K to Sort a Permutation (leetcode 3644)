// java

class Solution {
    public boolean isSorted(int[] nums){
        int n=nums.length;
        for(int i=1; i<n; i++){
            if(nums[i]<nums[i-1]) return false;
        }
        return true;
    }

    public int sortPermutation(int[] nums) {
        if(isSorted(nums)) return 0;

        int n=nums.length;
        int ans=-1;
        for(int i=0; i<n; i++){
            if(i!=nums[i]){
                ans=ans&nums[i];
            }
        }
        return ans;
    }
}
