// java

class Solution {
    public int[] getSneakyNumbers(int[] nums) {
        int n = nums.length;
        int[] freq = new int[n];
        for(int i=0; i<n; i++){
            freq[nums[i]]++;
        }

        List<Integer> list = new ArrayList<>();
        for(int i=0; i<n; i++) if(freq[i]>1) list.add(i);

        int[] result = new int[list.size()];
        for(int i=0; i<result.length; i++){
            result[i] = list.get(i);
        }
        return result;
    }
}
