// java

class Solution {
    public boolean isOneBitCharacter(int[] bits) {
        int n = bits.length;
        int i=0;
        boolean find = false;
        while(i<n){
            if(i==n-1 && bits[i]==0){
                find = true;
            }
            else if(bits[i]==1){
                i++;
            }
            i++;
        }
        return find;
    }
}
