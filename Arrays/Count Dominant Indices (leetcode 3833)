// java

class Solution {
    public int dominantIndices(int[] nums) {
        int n = nums.length;
        int suffix[] = new int[n];
        suffix[n-1] = nums[n-1];

        int count = 0;
        for(int i=n-2; i>=0; i--){
            suffix[i] = nums[i]+suffix[i+1];
            if(nums[i] > (suffix[i+1]/(n-(i+1)))){
                count++;
            }
        }

        return count;
    }
}
