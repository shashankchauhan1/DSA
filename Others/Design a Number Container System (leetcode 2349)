// java

class NumberContainers {
    Map<Integer,Integer> idxToNum;
    Map<Integer,TreeSet<Integer>> numberToIdx;
    public NumberContainers() {
        idxToNum = new HashMap<>();
        numberToIdx = new HashMap<>();
    }
    
    public void change(int index, int number) {
        if(!idxToNum.containsKey(index)){
            idxToNum.put(index,number);
            if(!numberToIdx.containsKey(number)) numberToIdx.put(number,new TreeSet<>());
            numberToIdx.get(number).add(index);
        }
        else{
            int oldnum = idxToNum.get(index);
            if(oldnum==number) return;
            numberToIdx.get(oldnum).remove(index);
            if(numberToIdx.get(oldnum).size()==0) numberToIdx.remove(oldnum);
            idxToNum.put(index,number);
            if(numberToIdx.containsKey(number)) numberToIdx.get(number).add(index);
            else{
                numberToIdx.put(number,new TreeSet<>());
                numberToIdx.get(number).add(index);
            }
        }
    }
    
    public int find(int number) {
        if(numberToIdx.containsKey(number)){
            if(numberToIdx.get(number).first()!=null){
                return numberToIdx.get(number).first();
            }
        }
        return -1;
    }
}

/**
 * Your NumberContainers object will be instantiated and called as such:
 * NumberContainers obj = new NumberContainers();
 * obj.change(index,number);
 * int param_2 = obj.find(number);
 */
