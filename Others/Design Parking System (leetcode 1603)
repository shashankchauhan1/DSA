// java

class ParkingSystem {
    int cntB;
    int cntM;
    int cntS;

    public ParkingSystem(int big, int medium, int small) {
        this.cntB = big;
        this.cntM = medium;
        this.cntS = small;
    }
    
    public boolean addCar(int carType) {
        if(carType == 1){
            if(cntB > 0) cntB--;
            else return false;
        }
        else if(carType == 2){
            if(cntM > 0) cntM--;
            else return false;
        }
        else if(carType == 3){
            if(cntS > 0) cntS--;
            else return false;
        }
        return true;
    }
}

/**
 * Your ParkingSystem object will be instantiated and called as such:
 * ParkingSystem obj = new ParkingSystem(big, medium, small);
 * boolean param_1 = obj.addCar(carType);
 */
