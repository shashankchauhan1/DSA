// java

/**
 * Definition for a binary tree node.
 * public class TreeNode {
 *     int val;
 *     TreeNode left;
 *     TreeNode right;
 *     TreeNode() {}
 *     TreeNode(int val) { this.val = val; }
 *     TreeNode(int val, TreeNode left, TreeNode right) {
 *         this.val = val;
 *         this.left = left;
 *         this.right = right;
 *     }
 * }
 */
class Solution {
    public void f(TreeNode root,int sum,int[] mainSum){
        if(root==null) return;

        sum=sum*10+root.val;
        if(root.left==null && root.right==null){
            mainSum[0]+=sum;
        }

        f(root.left,sum,mainSum);
        f(root.right,sum,mainSum);

    }

    public int sumNumbers(TreeNode root) {
        int[] mainSum=new int[1];
        mainSum[0]=0;
        f(root,0,mainSum);
        return mainSum[0];
    }
}
