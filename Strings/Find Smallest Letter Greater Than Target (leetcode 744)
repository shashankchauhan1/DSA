// java

class Solution {
    public char nextGreatestLetter(char[] letters, char target) {
        char ans = (char)(127);
        for(int i=0; i<letters.length; i++){
            if(letters[i] > target && letters[i] < ans){
                ans = letters[i];
            }
        }

        return ans==(char)(127) ? letters[0] : ans;
    }
}
