// java

class Solution {
    public boolean repeatedSubstringPattern(String s) {
        int n = s.length();

        for(int i=0; i<n-1; i++){
            String str = s.substring(0,i+1);
            int times = n/str.length();
            str = str.repeat(times);
            if(str.equals(s)) return true;
        }

        return false;
    }
}
