// java

class Solution {
    public List<Integer> toggleLightBulbs(List<Integer> bulbs) {
        List<Integer> list = new ArrayList<>();
        int n = bulbs.size();
        int[] freq = new int[101];
        for(int i=0; i<n; i++){
            freq[bulbs.get(i)]++;
        }

        for(int i=1; i<=100; i++){
            if(freq[i]%2!=0) list.add(i);
        }
        Collections.sort(list);

        return list;
    }
}
