// java

class Solution {
    public int findCenter(int[][] edges) {
        int n=edges.length+2;
        int[] indegree=new int[n];

        for(int[] edge:edges){
            indegree[edge[0]]++;
            indegree[edge[1]]++;
        }

        int max=-1;
        int ans=-1;
        for(int i=1; i<indegree.length; i++){
            if(indegree[i]>max){
                max=indegree[i];
                ans=i;
            }
        }

        return ans;
    }
}
