// java

class Solution {
    public int minNumberOperations(int[] target) {
        int n = target.length;

        int sum = 0;
        for(int i=1; i<n; i++){
            if(target[i] < target[i-1]) sum += target[i-1] - target[i];
        }
        sum += target[n-1];

        return sum;
    }
}
