// java

// Every n - 1 number contains all the set bits if n is the power of two
// 4 -> 3 (11)
// 16 -> 15 (111)

class Solution {
    public int smallestNumber(int n) {
        
        for(int i=n+1; i<=n*n+1; i++){
            if((i & (i-1))==0) return i-1;
        }

        return -1;
    }
}
