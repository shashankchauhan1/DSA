// java

class Solution {
    public int[] singleNumber(int[] nums) {
        int ans=0;
        for(int num:nums) ans=ans^num;

        int bit= ans & -ans;

        int a=0,b=0;

        for(int num:nums){
            if((num&bit) == 0) a=a^num;
            else b=b^num;
        }

        return new int[]{a,b};
    }
}
