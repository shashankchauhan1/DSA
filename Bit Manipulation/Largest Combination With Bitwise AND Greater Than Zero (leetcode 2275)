// java

class Solution {
    public int largestCombination(int[] candidates) {
        int maxCount=-1;
        for(int i=0; i<32; i++){
            int cnt=0;
            for(int num:candidates){
                if((num&(1<<i))!=0) cnt++;
            }
            maxCount=Math.max(maxCount,cnt);
        }
        return maxCount;
    }
}
