// java


class Solution {
    public int countSetBits(int n){
        int cnt=0;
        while(n!=0){
            n = (n & n-1);
            cnt++;
        }
        return cnt;
    }
    
    public int sumIndicesWithKSetBits(List<Integer> nums, int k) {
        int sum=0;
        int n=nums.size();

        for(int i=0; i<n; i++){
            int count=countSetBits(i);
            if(count==k){
                sum+=nums.get(i);
            }
        }

        return sum;
    }
}
