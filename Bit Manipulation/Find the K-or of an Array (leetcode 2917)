// java

class Solution {
    public int findKOr(int[] nums, int k) {
        int n=nums.length;
        int[] count=new int[32];

        for(int num:nums){
            for(int i=0; i<32; i++){
                if((num>>i & 1) == 1) count[i]++;
            }
        }

        int ans=0;
        for(int i=0; i<32; i++){
            if(count[i]>=k) ans = ans | (1<<i);
        }

        return ans;
    }
}
