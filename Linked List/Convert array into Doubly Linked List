// Convert array into Doubly Linked List

#include<iostream>
#include<bits/stdc++.h>
using namespace std;

class Node{
    public:
        int data;
        Node* next;
        Node* back;
        
        Node(int d,Node* n,Node* b){
            data=d;
            next=n;
            back=b;
        }
        
        Node(int d){
            data=d;
            next=nullptr;
            back=nullptr;
        }
};

Node* convertArray(vector<int>&v){
    Node* head=new Node(v[0]);
    Node* prev=head;
    for(int i=1; i<v.size(); i++){
        Node* temp=new Node(v[i]);
        temp->back=prev;
        prev->next=temp;
        prev=temp;
    }
    return head;
}

void printDLL(Node* head){
    Node* temp=head;
    while(temp){
        cout<<temp->data<<" ";
        temp=temp->next;
    }
    
}

int main(){
    vector<int>v={1,2,3,4};
    Node* head=convertArray(v);
    printDLL(head);
    return 0;
}
